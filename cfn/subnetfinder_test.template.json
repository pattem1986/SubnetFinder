{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Call SubnetFinder Custom Resource and give Subnet id as output",

    "Resources": {
        "AMIFinder": {
            "Type": "Custom::SubnetFinder",
            "Version": "1.0",
            "Properties": {
                "ServiceToken": "{{topic_arn}}",
                "VpcId": "{{vpc_id}}",
                "TagName" : "env",
                "TagValue" : "prod"
            }
        }
    },

    "Outputs" : {
        "SubnetId" : {
            "Value" : { "Ref" : "AMIFinder" }
        }
    }
}